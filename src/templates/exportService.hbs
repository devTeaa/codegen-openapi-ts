    {{{name}}}{{>isRequired}},{{/equals}}{{/each}}
  }: { {{#each parameters}}{{#equals in 'path'}}
    {{{name}}}{{>isRequired}}: {{>type}}{{#if default}} = {{{default}}}{{/if}}{{/equals}}{{/each}}
  }) => `{{path}}`,
  {{else}}
  {{capitalizeFirstLetter name}}: '{{path}}',
  {{/hasProperty}}
  {{/hasLength}}
  {{/each}}
}

